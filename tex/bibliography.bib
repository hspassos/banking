@article{cerettaAvaliacaoSelecaoFundos2001,
  title = {Avalia{\c c}{\~a}o e Sele{\c c}{\~a}o de Fundos de Investimento: Um Enfoque Sobre M{\'u}ltiplos Atributos},
  author = {Ceretta, Paulo Sergio and da Costa Jr., Newton C. A.},
  year = {2001},
  month = apr,
  journal = {Revista de Administra{\c c}{\~a}o Contempor{\^a}nea},
  volume = {5},
  number = {1},
  issn = {1415-6555},
  doi = {10.1590/S1415-65552001000100002},
  abstract = {{$<$}p{$>$}Este trabalho investiga o desempenho de fundos de investimento em a{\c c}{\~o}es mediante a an{\'a}lise por envolt{\'o}rio de dados. Essa ferramenta mostra-se poderosa na avalia{\c c}{\~a}o do desempenho de fundos de investimento, quando a fun{\c c}{\~a}o utilidade esperada do investidor apresenta m{\'u}ltiplos atributos. Desta maneira, al{\'e}m dos tradicionais atributos risco e retorno calculados sobre um determinado intervalo de tempo, a an{\'a}lise por envolt{\'o}rio de dados permite incorporar outros atributos importantes ao investidor, tais como custos de administra{\c c}{\~a}o, restri{\c c}{\~o}es de capital e mesmo os atributos risco e retorno calculados em per{\'i}odos mais recentes. Foram analisados 106 fundos na modalidade carteira livre, no per{\'i}odo de dezembro de 1997 a novembro de 1999. Os resultados obtidos identificaram sete fundos dominantes, os quais foram confrontados com os sete fundos menos eficientes, evidenciando-se suas diferen{\c c}as em termos de atributos e pondera{\c c}{\~o}es. Tamb{\'e}m foi realizada uma compara{\c c}{\~a}o em rela{\c c}{\~a}o ao {\'i}ndice de Sharpe. Cabe salientar que o principal objetivo do presente trabalho n{\~a}o {\'e} prescrever uma alternativa para o modelo de otimiza{\c c}{\~a}o m{\'e}dia-vari{\^a}ncia, mas o de descrever uma nova aplica{\c c}{\~a}o da an{\'a}lise por envolt{\'o}rio de dados.{$<$}/p{$>$}}
}

@article{fabiotakashialmenaraandakuPersistenciaDesempenhoDos2003,
  title = {A Persist{\^e}ncia de Desempenho Dos Fundos de Investimento Em A{\c c}{\~o}es No {{Brasil}}},
  author = {{Fabio Takashi Almenara Andaku} and {Antonio Carlos Figueiredo Pinto}},
  year = {2003},
  journal = {Revista de Economia e Administra{\c c}{\~a}o},
  volume = {2},
  number = {3},
  pages = {23--33},
  abstract = {Este artigo examina o desempenho de 84 fundos de a{\c c}{\~o}es brasileiros ao longo de um per{\'i}odo espec{\'i}fico (julho de 1994 a junho de 2001). A pesquisa foi feita como tentativa de verificar a exist{\^e}ncia de uma rela{\c c}{\~a}o direta entre desempenho passado dos fundos e o desempenho futuro prov{\'a}vel, o que {\'e} chamado usualmente de persist{\^e}ncia de desempenho. Os resultados do estudo mostraram a exist{\^e}ncia de persist{\^e}ncia de desempenho no mercado brasileiro para os investimentos de prazos mais longos, especificamente para aqueles maiores que um ano. Nos investimentos de curto prazo, entretanto, n{\~a}o foram obtidas evid{\^e}ncias suficientes para confirmar a persist{\^e}ncia de desempenho dos fundos.}
}

